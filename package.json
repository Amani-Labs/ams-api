    "apollo-server": "^2.10.1",
    "dotenv": "^8.2.0",
    "eslint-plugin-react": "^7.18.3",
    "graphql": "^14.6.0",
    "redis": "^3.0.2",
    "typescript": "^3.8.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.6",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "babel-jest": "^25.1.0",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "eslint": "^6.8.0",
